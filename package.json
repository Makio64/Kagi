{
  "name": "kagi-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run dev:frontend\"",
    "dev:backend": "cd backend && pnpm dev",
    "dev:frontend": "cd frontend && pnpm dev",
    "build": "pnpm run build:frontend",
    "build:frontend": "cd frontend && pnpm build",
    "install:all": "pnpm install && cd frontend && pnpm install",
    "lint": "cd frontend && pnpm lint",
    "test": "echo \"No tests yet\"",
    "storybook": "cd frontend && pnpm storybook",
    "build-storybook": "cd frontend && pnpm build-storybook"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "packageManager": "pnpm@10.24.0"
}
